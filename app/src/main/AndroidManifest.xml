<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hp.houseonrent">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-feature android:name="android.hardware.camera2.full" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/appico"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/appico_round"
        android:supportsRtl="true"
        android:theme="@style/SplashTheme">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUp" />
        <activity android:name=".Owner" />
        <activity android:name=".Login" />
        <activity android:name=".RegisterHouse" />
        <activity android:name=".Camera_HoR"></activity>
    </application>

</manifest>